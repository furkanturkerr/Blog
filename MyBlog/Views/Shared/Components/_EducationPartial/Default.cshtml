@model System.Collections.Generic.List<Entities.Concrate.Education>

@foreach (var item in Model)
{
    <li class="timeline-item">
        <h4 class="h4 timeline-item-title">@item.EducationContentlow</h4>
        <span>@item.EducationTime</span>
        <p class="timeline-text">@item.EducationText</p>
    </li>
}